<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://syssec.rub.de/SAAF" 
           xmlns="http://syssec.rub.de/SAAF" 
           elementFormDefault="qualified">
    <xs:element name="ad-networks">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="ad-network"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ad-network">
        <xs:complexType>
            <xs:attribute name="path-fragment" use="required"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
